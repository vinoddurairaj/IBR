[Unit]
Description=SFTKdtc stop (shutdown) script service hook for Linux kernels 3.7.0 and above
DefaultDependencies=no
After=halt-local.service umount.target
Before=final.target

[Service]
ExecStart=/usr/bin/true
RemainAfterExit=true
ExecStop=/etc/init.d/SFTKdtc-scan stop
[Install]
WantedBy=multi-user.target
