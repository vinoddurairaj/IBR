#!/bin/sh

#
# build HP-UX vers, according to brand
#

# parameters
prog=$0
seqnum=$1
brand=$2
count=$#
if [ $count = 3 ]; then
    log=$3
    $prog $seqnum $brand > $log 2>&1
    exit $?
fi


# environment
PATH=/usr/sbin:/opt/langtools/bin:/opt/LGTOdtc/bin:$PATH:.

# supporting tools
make=/usr/local/bin/gmake

# define OEM branding parameters
cd rebrand
${make} ${brand} seqnum=${seqnum}


cd ..; 
# build an installation package
${make} clean cdrom CDROMDIR=`pwd`/CDROMDIR

exit 0
