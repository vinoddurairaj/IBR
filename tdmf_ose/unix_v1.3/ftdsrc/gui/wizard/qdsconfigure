#!/usr/local/bin/wish8.0


source qdsnetwork.tcl
source qdsdialog.tcl
source wizard.tcl
source qdsconfigwiz.tcl
namespace import QdsNetwork::*

global Wizard::WIZ sock

toplevel .t
wm withdraw .t
wm withdraw .

if {$argc > 0} {
	if {[string compare [lindex $argv 1] "-offline"]} {
	 	set ::Wizard::WIZ(online) 0
	} 
} else {
		set ::Wizard::WIZ(online) 1
}	

Wizard::create .t ::QdsConfigWiz frame00

